{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 124",
    "not firefox <= 124",
    "not opera <= 109",
    "not ios_saf <= 17.4",
    "not safari <= 17.4"
  ],
  "hints": {
    "apple-touch-icons": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/structure": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "axe/keyboard": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "disown-opener": "off"
  }
}